{
  "name": "@stripemeter/replay-cli",
  "version": "0.1.0",
  "private": false,
  "bin": {
    "replay": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@stripemeter/core": "workspace:*",
    "axios": "^1.7.7",
    "commander": "^11.1.0",
    "csv-parse": "^5.5.6",
    "p-queue": "^7.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}

